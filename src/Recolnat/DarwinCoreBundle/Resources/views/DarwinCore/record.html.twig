{% extends 'RecolnatDarwinCoreBundle::template.html.twig' %}
{% block content %}
    <table class="table table-striped table-bordered table-hover table-condensed">
        {% for name,value in data.getDatasWithIndexes %}
        	<tr>
        		<th>
        		{{ name }}
        		</th>
        		<td>
         		{{ value }}
        		</td>
        	</tr>
    	{% endfor %}
    </table>
{% endblock %}